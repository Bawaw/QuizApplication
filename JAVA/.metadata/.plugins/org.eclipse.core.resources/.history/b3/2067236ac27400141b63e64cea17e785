package domain;

public class TestHandler {
	private Evaluation test;
	
	public TestHandler() {
		
	}

	public Evaluation getTest() {
		return test;
	}

	public void setTest(Evaluation test) throws DomainException {
		if(test==null){
			throw new DomainException("Test cannot be null");
		}
		this.test = test;
	}

	
}
