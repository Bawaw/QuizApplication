package domain;

import java.util.HashSet;

public class YesNoQuestion extends Question {

	
	public YesNoQuestion(String question, Answer rightAnswer, int time) throws DomainException {
		HashSet<Answer> answers = new HashSet<Answer>();
		answers.add(new Answer("yes"));
		answers.add(new Answer("no"));
		super(question, rightAnswer, answers, time);
		
	}

}
