	}

	public RemoveFeedbackAction(FacadeActionManager service, String caption) {
		super(service, caption);
		// TODO Auto-generated constructor stub
	}

	@Override
	public void actionPerformed(ActionEvent e) {
		String newFeedbak = getCategoryFeedbackPanel().getNewFeedbackFieldText();
		getService();
	}

	public CategoryDetailPanel getCategoryFeedbackPanel() {
		return categoryFeedbackPanel;
	}

	public void setCategoryFeedbackPanel(CategoryDetailPanel categoryFeedbackPanel) {
		this.categoryFeedbackPanel = categoryFeedbackPanel;
	}
	
	
}
